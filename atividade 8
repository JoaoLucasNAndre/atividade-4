programa
{
	
	funcao inicio()
	{
		escreva("Me informe sua senha: ")
		cadeia senha
		leia(senha)
		
		enquanto(senha=="12345" ou senha=="admin" ou senha=="senha" ou senha=="xuxa"){
		escreva("Senha insegura! ")

		escreva("Informe outra senha: ")
		leia(senha)
		
		}
		escreva("Muito obrigada!")
		
	}
}
